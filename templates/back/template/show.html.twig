<!DOCTYPE html>

<title>Template</title>

{% block body %}
    <h1>Template</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ template.id }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ template.image }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ template.content }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('back_app_template_index') }}">back to list</a>

    <a href="{{ path('back_app_template_edit', {'id': template.id}) }}">edit</a>

    {{ include('back/template/_delete_form.html.twig') }}
{% endblock %}
